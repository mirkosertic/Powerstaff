<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="de.mogwai.common.business.entity">
	<class name="LockEntry" table="T_LOCKTABLE">
		<id name="id" type="long" column="ID" unsaved-value="null">
			<generator class="increment"/>
		</id>

		<property name="lockDate" column="LOCKDATE"
			not-null="false">
		</property>
		
		<property name="lockUser" column="LOCKUSER"
			not-null="false">
		</property>

		<property name="objectId">
			<column name="OBJECT_ID" not-null="true" />
		</property>

		<property name="objectName">
			<column name="OBJECTNAME" length="100" not-null="true" />
		</property>
		
		<property name="sessionId">
			<column name="SESSION_ID" not-null="true" />
		</property>

	</class>
</hibernate-mapping>
