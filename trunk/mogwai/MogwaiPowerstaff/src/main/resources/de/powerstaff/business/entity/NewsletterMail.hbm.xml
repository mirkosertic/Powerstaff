<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="de.powerstaff.business.entity" default-lazy="false">
  <class name="NewsletterMail" table="newslettermail">

		<id name="mail" column="mail">
	        <generator class="assigned"/>
		</id>

		<property name="registeredTimestamp"/>
		<property name="confirmed"/>
		<property name="confirmedTimestamp"/>
		<property name="errorcounter"/>

  </class>
</hibernate-mapping>