<pretty-config
        xmlns="http://ocpsoft.com/prettyfaces-xsd"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://ocpsoft.com/prettyfaces-xsd
	http://ocpsoft.com/xml/ns/prettyfaces/pretty-1.0.xsd">
    <url-mapping id="login">
        <pattern>/login</pattern>
        <view-id>/login.faces</view-id>
    </url-mapping>
    <url-mapping id="freelancer">
        <pattern>/freelancer/#{freelancerBackingBean.data.currentFreelancerId}</pattern>
        <view-id>/include/freelancer/freelancermain.faces</view-id>
        <action onPostback="false">#{freelancerBackingBean.loadFreelancer}</action>
    </url-mapping>
    <url-mapping id="partner">
        <pattern>/partner/#{partnerBackingBean.data.currentPartnerId}</pattern>
        <view-id>/include/partner/partnermain.faces</view-id>
        <action onPostback="false">#{partnerBackingBean.loadPartner}</action>
    </url-mapping>
    <url-mapping id="customer">
        <pattern>/customer/#{customerBackingBean.data.currentCustomerId}</pattern>
        <view-id>/include/customer/customermain.faces</view-id>
        <action onPostback="false">#{customerBackingBean.loadCustomer}</action>
    </url-mapping>
    <url-mapping id="project">
        <pattern>/project/#{projectBackingBean.data.currentProjectId}</pattern>
        <view-id>/include/project/projectmain.faces</view-id>
        <action onPostback="false">#{projectBackingBean.loadProject}</action>
    </url-mapping>
</pretty-config>