<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN" "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">
<faces-config>

	<application>
		<view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
		<variable-resolver>
			de.mogwai.common.web.resolver.BeanInitingVariableResolver
		</variable-resolver>
		<navigation-handler>
			de.mogwai.common.web.navigation.NavigationHandler
		</navigation-handler>
		<action-listener>
			de.mogwai.common.web.actionlistener.ValidatingActionListener
		</action-listener>
	</application>

	<converter>
		<converter-id>contactConverter</converter-id>
		<converter-class>
			de.powerstaff.web.converter.ContactConverter
		</converter-class>
	</converter>

	<converter>
		<converter-id>auditInformationConverter</converter-id>
		<converter-class>
			de.powerstaff.web.converter.AuditInformationConverter
		</converter-class>
	</converter>

	<converter>
		<converter-id>creationAuditInformationConverter</converter-id>
		<converter-class>
			de.powerstaff.web.converter.CreationAuditInformationConverter
		</converter-class>
	</converter>

	<converter>
		<converter-id>personConverter</converter-id>
		<converter-class>
			de.powerstaff.web.converter.PersonConverter
		</converter-class>
	</converter>

	<converter>
		<converter-id>textConverter</converter-id>
		<converter-class>
			de.powerstaff.web.converter.TextConverter
		</converter-class>
	</converter>

	<managed-bean>
		<managed-bean-name>navigationBackingBean</managed-bean-name>
		<managed-bean-class>
			de.powerstaff.web.backingbean.navigation.NavigationBackingBean
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>freelancerBackingBean</managed-bean-name>
		<managed-bean-class>
			de.powerstaff.web.backingbean.freelancer.FreelancerBackingBean
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>entityService</property-name>
			<value>#{freelancerService}</value>
		</managed-property>
		<managed-property>
			<property-name>partnerService</property-name>
			<value>#{partnerService}</value>
		</managed-property>
		<managed-property>
			<property-name>additinalDataService</property-name>
			<value>#{additinalDataService}</value>
		</managed-property>
		<managed-property>
			<property-name>profileSearchService</property-name>
			<value>#{profileSearchService}</value>
		</managed-property>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>customerBackingBean</managed-bean-name>
		<managed-bean-class>
			de.powerstaff.web.backingbean.customer.CustomerBackingBean
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>entityService</property-name>
			<value>#{customerService}</value>
		</managed-property>
		<managed-property>
			<property-name>additinalDataService</property-name>
			<value>#{additinalDataService}</value>
		</managed-property>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>partnerBackingBean</managed-bean-name>
		<managed-bean-class>
			de.powerstaff.web.backingbean.partner.PartnerBackingBean
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>entityService</property-name>
			<value>#{partnerService}</value>
		</managed-property>
		<managed-property>
			<property-name>additinalDataService</property-name>
			<value>#{additinalDataService}</value>
		</managed-property>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>projectBackingBean</managed-bean-name>
		<managed-bean-class>
			de.powerstaff.web.backingbean.project.ProjectBackingBean
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>entityService</property-name>
			<value>#{projectService}</value>
		</managed-property>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>profileBackingBean</managed-bean-name>
		<managed-bean-class>
			de.powerstaff.web.backingbean.profile.ProfileBackingBean
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>profileSearchService</property-name>
			<value>#{profileSearchService}</value>
		</managed-property>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>loginBackingBean</managed-bean-name>
		<managed-bean-class>
			de.powerstaff.web.backingbean.login.LoginBackingBean
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>loginService</property-name>
			<value>#{loginService}</value>
		</managed-property>
	</managed-bean>

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>FREELANCER_STAMMDATEN</from-outcome>
			<to-view-id>
				/include/freelancer/freelancermain.xhtml
			</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>FREELANCER_HISTORIE</from-outcome>
			<to-view-id>
				/include/freelancer/freelancerhistory.xhtml
			</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>FREELANCER_SEARCHRESULT</from-outcome>
			<to-view-id>
				/include/freelancer/freelancersearchresult.xhtml
			</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>CUSTOMER_STAMMDATEN</from-outcome>
			<to-view-id>
				/include/customer/customermain.xhtml
			</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>CUSTOMER_HISTORIE</from-outcome>
			<to-view-id>
				/include/customer/customerhistory.xhtml
			</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>CUSTOMER_SEARCHRESULT</from-outcome>
			<to-view-id>
				/include/customer/customersearchresult.xhtml
			</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>PARTNER_STAMMDATEN</from-outcome>
			<to-view-id>/include/partner/partnermain.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>PARTNER_HISTORIE</from-outcome>
			<to-view-id>
				/include/partner/partnerhistory.xhtml
			</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>PARTNER_SEARCHRESULT</from-outcome>
			<to-view-id>
				/include/partner/partnersearchresult.xhtml
			</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>PROJEKT_STAMMDATEN</from-outcome>
			<to-view-id>/include/project/projectmain.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>PROJEKT_SEARCHRESULT</from-outcome>
			<to-view-id>
				/include/project/projectsearchresult.xhtml
			</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>PROFILE_STAMMDATEN</from-outcome>
			<to-view-id>/include/profile/profilemain.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>INDEX</from-outcome>
			<to-view-id>
				/include/freelancer/freelancermain.xhtml
			</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>

