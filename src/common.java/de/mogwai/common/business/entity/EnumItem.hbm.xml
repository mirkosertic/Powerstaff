<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="de.mogwai.common.business.entity">
	<class name="EnumItem" table="T_ENUM_ITEM">
		<id name="id" type="long" column="ID" unsaved-value="null">
			<generator class="increment"/>
		</id>
		
		<property name="creationDate" column="CREATIONDATE"
			not-null="false">
		</property>
		<property name="creationUserID" column="CREATIONUSERID"
			not-null="false">
		</property>
		<property name="lastModificationDate"
			column="LASTMODIFICATIONDATE" not-null="false">
		</property>
		<property name="lastModificationUserID"
			column="LASTMODIFICATIONUSERID" not-null="false">
		</property>
		
		<property name="listIndex" type="integer" column="LIST_INDEX"
			not-null="false" />
		<property name="name" type="string" length="50" column="NAME"
			not-null="true" />
		<map name="labels" table="T_ENUM_ITEM_LABEL" lazy="false"
			fetch="join">
			<key column="ENUM_ITEM_ID" />
			<map-key type="string" length="2" column="LANGUAGE_CODE" />
			<element type="string" length="200" column="TEXT" />
		</map>
	</class>
</hibernate-mapping>